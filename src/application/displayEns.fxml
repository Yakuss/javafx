<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.control.Button?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.DisplayEnsController">
    <children>
        <TableView fx:id="tableView" layoutX="14.0" layoutY="14.0" prefHeight="200.0" prefWidth="600.0">
            <columns>
                <TableColumn fx:id="idColumn" text="ID" />
                <TableColumn fx:id="nameColumn" text="Name" />
                <TableColumn fx:id="prenomColumn" text="Prenom" />
                <TableColumn fx:id="numMobColumn" text="Mobile" />
                <TableColumn fx:id="emailColumn" text="Email" />
                <TableColumn fx:id="departementColumn" text="Departement" />
                <TableColumn fx:id="updateColumn" text="Update" />
                <TableColumn fx:id="deleteColumn" text="Delete" />
            </columns>
        </TableView>
    </children>
</AnchorPane>
